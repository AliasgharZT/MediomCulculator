<Style>:
    txt:txt
    mn:mn 
    _fpl:fpl_ 
    _xy:xy_ 
    _xyz:xyz_
    _integ:integ_
    MDScreenManager:
        id:mn 
# /////////MAIN///////////
        MDScreen:
            name:'main'
            MDScreen:
                MDAnchorLayout:
                    anchor_x:'center'
                    anchor_y:'center'
                    MDBoxLayout:
                        orientation: 'vertical'
                        padding:5
                        spacing:3
                        size_hint:0.5,0.6
                        MDLabel:
                            id:txt  
                            text:''
                            size_hint: 1.06, 0.1
                            halign:'center'
                            md_bg_color: [1,1,1,0.5]
                        MDBoxLayout:
                            orientation: 'horizontal'
                            MDGridLayout:
                                cols:6
                                spacing:6
                                MDRectangleFlatButton:
                                    id:btn1
                                    text:'1'
                                    on_press:txt.text+=btn1.text 
                                MDRectangleFlatButton:
                                    id:btn2
                                    text:'2'
                                    on_press:txt.text=txt.text+btn2.text
                                MDRectangleFlatButton:
                                    id:btn3
                                    text:'3'
                                    on_press:txt.text=txt.text+btn3.text
                                MDRectangleFlatButton:
                                    id:btn4
                                    text:'4'
                                    on_press:txt.text=txt.text+btn4.text
                                MDRectangleFlatButton:
                                    text:'¶'
                                    text_color: "white"
                                    on_press:root.clear1()
                                MDRectangleFlatButton:
                                    text:'c'
                                    text_color: "white"
                                    on_press:txt.text=''
                                MDRectangleFlatButton:
                                    id:btn5
                                    text:'5'
                                    on_press:txt.text=txt.text+btn5.text
                                MDRectangleFlatButton:
                                    id:btn6
                                    text:'6'
                                    on_press:txt.text=txt.text+btn6.text
                                MDRectangleFlatButton:
                                    id:btn7
                                    text:'7'
                                    on_press:txt.text=txt.text+btn7.text
                                MDRectangleFlatButton:
                                    id:btn8
                                    text:'8'
                                    on_press:txt.text=txt.text+btn8.text
                                MDRectangleFlatButton:
                                    text:'x'
                                    text_color: "white"
                                    on_press:root.zar()
                                MDRectangleFlatButton:
                                    text:'/'
                                    text_color: "white"
                                    on_press:root.tagh()
                                MDRectangleFlatButton:
                                    id:btn9
                                    text:'9'
                                    on_press:txt.text=txt.text+btn9.text
                                MDRectangleFlatButton:
                                    id:btn0
                                    text:'0'
                                    on_press:txt.text=txt.text+btn0.text
                                MDRectangleFlatButton:
                                    text:'.'
                                    on_press:root.mom() 
                                MDRectangleFlatButton:
                                    text:'+/-'
                                    text_color:'white'
                                    on_press:root.mosman()
                                MDRectangleFlatButton:
                                    text:'-'
                                    text_color: "white"
                                    on_press:root.kam()
                                MDRectangleFlatButton:
                                    text:'√'
                                    text_color: "white"
                                    on_press:root.radic()
                                MDRectangleFlatButton:
                                    text:'sin'
                                    text_color:'green'
                                    on_press:root.sin()
                                MDRectangleFlatButton:
                                    text:'cos'
                                    text_color:'green'
                                    on_press:root.cos()
                                MDRectangleFlatButton:
                                    text:'tan'
                                    text_color:'green'
                                    on_press:root.tan()
                                MDRectangleFlatButton:
                                    text:'cot'
                                    text_color:'green'
                                    on_press:root.cot()
                                MDRectangleFlatButton:
                                    text:'+'
                                    text_color: "white"
                                    on_press:root.jam()
                                MDRectangleFlatButton:
                                    text:'='
                                    text_color: "white"
                                    on_press:root.result()
                                MDRectangleFlatButton:
                                    text:'cosh'
                                    text_color:'green'
                                    on_press:root.cosh()
                                MDRectangleFlatButton:
                                    text:'sinh'
                                    text_color:'green'
                                    on_press:root.sinh()
                                MDRectangleFlatButton:
                                    text:'tanh'
                                    text_color:'green'
                                    on_press:root.tanh()
                                MDRectangleFlatButton:
                                    text:'coth'
                                    text_color:'green'
                                    on_press:root.coth()

            MDScreen:
                MDFloatingActionButtonSpeedDial:
                    root_button_anim: True
                    data:{'Fac!,Pow^,log_a':['calculator',"on_press",lambda x:root.fpl()],'{xy=c/xy=c}':['code-not-equal-variant',"on_press",lambda x:root.xy()],'{xyz=c/xyz=c/xyz=c}':['equalizer',"on_press",lambda x:root.xyz()],'(a*x){+-*^}(x^a)...-c\n>>(2*x)+(x^8)+(2*5)-6':['abacus',"on_press",lambda x:root.integ()]}
# ////////FAKE///////////
        MDScreen:
            name:'fpl'
            MDAnchorLayout:
                anchor_x:'center'
                anchor_y:'center'
                MDBoxLayout:
                    orientation: 'vertical'
                    size_hint: 0.5, 0.2
                    MDTextField:
                        id:fpl_
                        mode:'round'
                    MDAnchorLayout:
                        anchor_x:'center'
                        anchor_y:'center'
                        MDGridLayout:
                            cols:3
                            padding:3
                            spacing:5
                            MDRectangleFlatButton:
                                text:'fact'
                                on_press:root.fact()
                            MDRectangleFlatButton:
                                text:'Pow'
                                on_press:root.power()
                            MDRectangleFlatButton:
                                text:'Log'
                                on_press:root.loga()
                            MDRectangleFlatButton:
                                text:'Clear'
                                text_color:'green'
                                on_press:fpl_.text=''
                            MDRectangleFlatButton:
                                text:'Back'
                                text_color:'white'
                                on_press:mn.current='main'   
        MDScreen:
            name:'xy'
            MDAnchorLayout:
                anchor_x:'center'
                anchor_y:'center'
                MDBoxLayout:
                    orientation: 'vertical'
                    size_hint: 0.5, 0.2
                    MDTextField:
                        id:xy_
                        mode:'round'
                    MDAnchorLayout:
                        anchor_x:'center'
                        anchor_y:'center'
                        MDGridLayout:
                            cols:2
                            padding:3
                            spacing:5
                            MDRectangleFlatButton:
                                text:'Clear'
                                text_color:'green'
                                on_press:xy_.text=''
                            MDRectangleFlatButton:
                                text:'Solve'
                                on_press:root.solve_xy()
                            MDRectangleFlatButton:
                                text:'Back'
                                text_color:'white'
                                on_press:mn.current='main'   
        MDScreen:
            name:'xyz'
            MDAnchorLayout:
                anchor_x:'center'
                anchor_y:'center'
                MDBoxLayout:
                    orientation: 'vertical'
                    size_hint: 0.5, 0.2
                    MDTextField:
                        id:xyz_
                        mode:'round'
                    MDAnchorLayout:
                        anchor_x:'center'
                        anchor_y:'center'
                        MDGridLayout:
                            cols:2
                            padding:3
                            spacing:5
                            MDRectangleFlatButton:
                                text:'Clear'
                                text_color:'green'
                                on_press:xyz_.text=''
                            MDRectangleFlatButton:
                                text:'Solve'
                                on_press:root.solve_xyz()
                            MDRectangleFlatButton:
                                text:'Back'
                                text_color:'white'
                                on_press:mn.current='main'  
        MDScreen:
            name:'integ'
            MDAnchorLayout:
                anchor_x:'center'
                anchor_y:'center'
                MDBoxLayout:
                    orientation: 'vertical'
                    size_hint: 0.5, 0.2
                    MDTextField:
                        id:integ_
                        mode:'round'
                    MDAnchorLayout:
                        anchor_x:'center'
                        anchor_y:'center'
                        MDGridLayout:
                            cols:2
                            padding:3
                            spacing:5
                            MDRectangleFlatButton:
                                text:'Clear'
                                text_color:'green'
                                on_press:integ_.text=''
                            MDRectangleFlatButton:
                                text:'Solve'
                                on_press:root.solve_integ()
                            MDRectangleFlatButton:
                                text:'Back'
                                text_color:'white'
                                on_press:mn.current='main'
